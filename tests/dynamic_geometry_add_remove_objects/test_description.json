{
  "description": "Test if meshes can be added and removed after a dynamic geometry event.",
  "author": "Jacob Czech",
  "date": "2014-12-10",
  "keywords": [
    "dynamic geometry"
  ],
  "run": {
      "mdlfiles": ["add_remove_objects.mdl"],
      "commandlineOpts": [
        "-quiet"
      ]
  },
  "checks": [
    {
      "testType": "CHECK_SUCCESS"
    },
    {
       "testType": "DIFF_FILE_CONTENT",
       "dataFile": "vm_1_A.dat",
       "templateFile": "vm_1_A_ref.dat"
    },
    {
       "testType": "DIFF_FILE_CONTENT",
       "dataFile": "vm_1_B.dat",
       "templateFile": "vm_1_B_ref.dat"
    },
    {
       "testType": "DIFF_FILE_CONTENT",
       "dataFile": "vm_1_C.dat",
       "templateFile": "vm_1_C_ref.dat"
    },
    {
       "testType": "DIFF_FILE_CONTENT",
       "dataFile": "vm_1_D.dat",
       "templateFile": "vm_1_D_ref.dat"
    },
    {
      "testType": "COMPARE_COUNTS",
      "dataFile": "vm_1_world.dat",
      "referenceFile": "vm_1_world_ref.dat"
    }
  ]
}
