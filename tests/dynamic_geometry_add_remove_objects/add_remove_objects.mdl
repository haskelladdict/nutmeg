ITERATIONS = 15
TIME_STEP = 1e-6

WARNINGS
{
  LARGE_MOLECULAR_DISPLACEMENT = IGNORED
}

DYNAMIC_GEOMETRY = "./geom.txt"

DEFINE_MOLECULES
{
  vm { DIFFUSION_CONSTANT_3D = 0 }
}

INCLUDE_FILE = "./geom1.mdl"

INSTANTIATE Scene OBJECT
{
  A OBJECT A{}
  B OBJECT B{}
  C OBJECT C{}
  rel_vm RELEASE_SITE
  {
    SHAPE = Scene.A
    MOLECULE = vm
    NUMBER_TO_RELEASE = 1000
    SITE_DIAMETER = 0.0
    LOCATION = [0.0, 0.0, 0.0]
  }
}

REACTION_DATA_OUTPUT
{
  STEP=1e-06
  HEADER=OFF
  {COUNT[vm,WORLD]}=> "./vm_world.dat"
  {COUNT[vm,Scene.A]}=> "./vm_A.dat"
  {COUNT[vm,Scene.B]}=> "./vm_B.dat"
  {COUNT[vm,Scene.C]}=> "./vm_C.dat"
  {COUNT[vm,Scene.D]}=> "./vm_D.dat"
}
