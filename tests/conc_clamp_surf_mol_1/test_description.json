{
    "description": "This test checks that a surface clamp on a region for surface molecules releases molecules on the correct side of the region boundary",
    "author": "Markus Dittrich <dittrich@psc.edu>",
    "date": "2014-12-11",
    "keywords": [
        "releases"
    ],
    "run": {
        "mdlfiles": [
            "test.mdl"
        ],
        "commandlineOpts": [
            "-quiet"
        ]
    },
    "checks": [
        {
            "testType": "CHECK_SUCCESS"
        },
        {   "testType": "ZERO_COUNTS",
            "dataFile": "A_outside.dat"
        },
        {   "testType": "ZERO_COUNTS",
            "dataFile": "B_inside.dat"
        }
    ]
}
