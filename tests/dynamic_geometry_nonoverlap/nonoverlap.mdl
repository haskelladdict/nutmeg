ITERATIONS = 10
TIME_STEP = 1e-6

WARNINGS
{
  LARGE_MOLECULAR_DISPLACEMENT = IGNORED
}

DYNAMIC_GEOMETRY = "./list_of_geometry.txt"

DEFINE_MOLECULES
{
  vm1 { DIFFUSION_CONSTANT_3D = 0 }
  vm2 { DIFFUSION_CONSTANT_3D = 0 }
  vm3 { DIFFUSION_CONSTANT_3D = 0 }
  vm4 { DIFFUSION_CONSTANT_3D = 0 }
  vm5 { DIFFUSION_CONSTANT_3D = 0 }
}

INCLUDE_FILE = "./geom_1.mdl"

DEFINE_SURFACE_CLASSES
{
  transp_out_vm2345
  {
    TRANSPARENT = vm2,
    TRANSPARENT = vm3,
    TRANSPARENT = vm4,
    TRANSPARENT = vm5,
  }
  transp_out_vm345
  {
    TRANSPARENT = vm3,
    TRANSPARENT = vm4,
    TRANSPARENT = vm5,
  }
  transp_in_45
  {
    TRANSPARENT = vm4'
    TRANSPARENT = vm5'
  }
  transp_in_5
  {
    TRANSPARENT = vm5'
  }
}

MODIFY_SURFACE_REGIONS
{
  Linner[ALL]
  {
    SURFACE_CLASS = transp_out_vm2345
  }
  Louter[ALL]
  {
    SURFACE_CLASS = transp_out_vm345
  }
  Router[ALL]
  {
    SURFACE_CLASS = transp_in_45
  }
  Rinner[ALL]
  {
    SURFACE_CLASS = transp_in_5
  }
}

INSTANTIATE Scene OBJECT
{
  Linner OBJECT Linner{}
  Louter OBJECT Louter{}
  Rinner OBJECT Rinner{TRANSLATE = [0.21, 0, 0]}
  Router OBJECT Router{TRANSLATE = [0.21, 0, 0]}
  rel_vm1 RELEASE_SITE
  {
    SHAPE = CUBIC
    MOLECULE = vm1
    NUMBER_TO_RELEASE = 100
    SITE_DIAMETER = 0.0
    LOCATION = [0.0, 0.0, 0.0]
  }
  rel_vm2 RELEASE_SITE
  {
    SHAPE = CUBIC
    MOLECULE = vm2
    NUMBER_TO_RELEASE = 100
    SITE_DIAMETER = 0.0
    LOCATION = [0.0, 0.0, 0.0]
  }
  rel_vm3 RELEASE_SITE
  {
    SHAPE = CUBIC
    MOLECULE = vm3
    NUMBER_TO_RELEASE = 100
    SITE_DIAMETER = 0.0
    LOCATION = [0.0, 0.0, 0.0]
  }
  rel_vm4 RELEASE_SITE
  {
    SHAPE = CUBIC
    MOLECULE = vm4
    NUMBER_TO_RELEASE = 100
    SITE_DIAMETER = 0.0
    LOCATION = [0.0, 0.0, 0.0]
  }
  rel_vm5 RELEASE_SITE
  {
    SHAPE = CUBIC
    MOLECULE = vm5
    NUMBER_TO_RELEASE = 100
    SITE_DIAMETER = 0.0
    LOCATION = [0.0, 0.0, 0.0]
  }
}

REACTION_DATA_OUTPUT
{
  STEP=1e-6
  // vm1
  {COUNT[vm1,Scene.Linner],
  COUNT[vm1,Scene.Louter]-COUNT[vm1,Scene.Linner],
  COUNT[vm1,Scene.Rinner],
  COUNT[vm1,Scene.Router]-COUNT[vm1,Scene.Rinner],
  COUNT[vm1,WORLD]-COUNT[vm1,Scene.Louter]-COUNT[vm1,Scene.Router]}=> "./vm1.dat"
  // vm2
  {COUNT[vm2,Scene.Linner],
  COUNT[vm2,Scene.Louter]-COUNT[vm2,Scene.Linner],
  COUNT[vm2,Scene.Rinner],
  COUNT[vm2,Scene.Router]-COUNT[vm2,Scene.Rinner],
  COUNT[vm2,WORLD]-COUNT[vm2,Scene.Louter]-COUNT[vm2,Scene.Router]}=> "./vm2.dat"
  // vm3
  {COUNT[vm3,Scene.Linner],
  COUNT[vm3,Scene.Louter]-COUNT[vm3,Scene.Linner],
  COUNT[vm3,Scene.Rinner],
  COUNT[vm3,Scene.Router]-COUNT[vm3,Scene.Rinner],
  COUNT[vm3,WORLD]-COUNT[vm3,Scene.Louter]-COUNT[vm3,Scene.Router]}=> "./vm3.dat"
  // vm4
  {COUNT[vm4,Scene.Linner],
  COUNT[vm4,Scene.Louter]-COUNT[vm4,Scene.Linner],
  COUNT[vm4,Scene.Rinner],
  COUNT[vm4,Scene.Router]-COUNT[vm4,Scene.Rinner],
  COUNT[vm4,WORLD]-COUNT[vm4,Scene.Louter]-COUNT[vm4,Scene.Router]}=> "./vm4.dat"
  // vm5
  {COUNT[vm5,Scene.Linner],
  COUNT[vm5,Scene.Louter]-COUNT[vm5,Scene.Linner],
  COUNT[vm5,Scene.Rinner],
  COUNT[vm5,Scene.Router]-COUNT[vm5,Scene.Rinner],
  COUNT[vm5,WORLD]-COUNT[vm5,Scene.Louter]-COUNT[vm5,Scene.Router]}=> "./vm5.dat"
}

