TIME_STEP     = 1e-6
ITERATIONS    = 20

DEFINE_MOLECULES {
  A { DIFFUSION_CONSTANT = 5e-6 }
  B { DIFFUSION_CONSTANT_3D = 5e-6 }
}

DEFINE_SURFACE_CLASSES {
  clamp { 
    CLAMP_CONC A, = 2 
    CLAMP_CONC B' = 1
  }
}

DYNAMIC_GEOMETRY = "./geom.txt"

INCLUDE_FILE = "geom1.mdl"

MODIFY_SURFACE_REGIONS
{
  Cube[ALL]
  {
    SURFACE_CLASS = clamp
  }
}

INSTANTIATE Scene OBJECT 
{
  Cube OBJECT Cube {}
  BiggerCube OBJECT BiggerCube {}
}

REACTION_DATA_OUTPUT {
  STEP = 1e-6
  {COUNT [A,Scene.Cube] - COUNT[A,WORLD]} => "./A_outside.dat"
  {COUNT [B,Scene.Cube]} => "./B_inside.dat"
}
